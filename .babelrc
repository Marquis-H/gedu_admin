{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "safari >= 7",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-react-jsx",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-runtime",
    "transform-vue-jsx",
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ]
}